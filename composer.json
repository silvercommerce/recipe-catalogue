{
    "name": "silvercommerce/recipe-catalogue",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for product catalogue management",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-core": "^4.0",
        "silverstripe/admin": "1.*@stable",
        "silverstripe/asset-admin": "1.*@stable",
        "silverstripe/siteconfig": "4.*@stable",
        "silverstripe/versioned": "1.*@stable",
        "silvercommerce/catalogue-admin": "1.*@dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "project-files": [
            "mysite/code/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
